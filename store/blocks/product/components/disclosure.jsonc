	"disclosure-layout#faq": {
		"props": {
			"blockClass": ["accordion-container"]
		},
		"children": ["disclosure-trigger#faq", "disclosure-content#faq"]
	},
	"disclosure-trigger#faq": {
		"children": ["rich-text#question-faq", "disclosure-state-indicator"]
	},
	"disclosure-content#faq": {
		"children": [
			"disclosure-layout#faq-1",
			"disclosure-layout#faq-2",
			"disclosure-layout#faq-3",
			"disclosure-layout#faq-4"
		]
	},

	"flex-layout.col#about-product": {
		"children": [
			"disclosure-layout#about-product-1",
			"disclosure-layout#about-product-2",
			"disclosure-layout#about-product-3"
		]
	},

	"flex-layout.col#faq": {
		"children": [
			"rich-text#question-faq",
			"disclosure-layout#faq-1",
			"disclosure-layout#faq-2",
			"disclosure-layout#faq-3",
			"disclosure-layout#faq-4"
		]
	},

	"rich-text#question-faq": {
		"props": {
			"text": "## DUVIDAS? **TE AJUDAMOS**",
			"blockClass": ["faq-title"]
		}
	},

	"disclosure-state-indicator": {
		"props": {
			"Show": "image#arrow-down",
			"Hide": "image#arrow-up"
		}
	},
	"image#arrow-up": {
		"props": {
			"src": "assets/img/icons/up.svg"
		}
	},

	"image#arrow-down": {
		"props": {
			"src": "assets/img/icons/down.svg"
		}
	}
}
