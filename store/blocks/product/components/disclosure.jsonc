			"flex-layout.col#faq"
		]
	},

	"disclosure-layout#info-nutricional": {
		"props": {
			"blockClass": ["accordion-container"]
		},
		"children": [
			"disclosure-trigger#info-nutricional",
			"disclosure-content#info-nutricional"
		]
	},
	"disclosure-trigger#info-nutricional": {
		"children": [
			"rich-text#question-info-nutricional",
			"disclosure-state-indicator"
		]
	},
	"disclosure-content#info-nutricional": {
		"children": ["rich-text#answer-info-nutricional"]
	},
	"rich-text#question-info-nutricional": {
		"props": {
			"text": "__Informação Nutricional__",
			"blockClass": ["accordion-info"]
		}
	},
	"rich-text#answer-info-nutricional": {
		"props": {
			"text": "Informação Nutricional"
		}
	},

