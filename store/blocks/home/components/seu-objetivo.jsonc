	"responsive-layout.mobile#qual-seu-objetivo": {
		"children": ["flex-layout.row#content-seu-objetivo-mobile"]
	},

	"flex-layout.row#content-seu-objetivo-mobile": {
		"title": "[Mobile] Container - Qual seu objetivo ",
		"props": {
			"blockClass": "container-qual-seu-objetivo"
		},
		"children": [
			"link#respiracao",
			"link#movimento",
			"link#alimentacao",
			"link#sono"
		]
	},

	"link#respiracao": {
		"title": "[Mobile] Texto 1",
		"props": {
			"label": "Respiração",
			"href": "/respiracao",
			"blockClass": ["qual-seu-objetivo-mobile"]
		}
	},

	"link#movimento": {
		"title": "[Mobile] Texto 2",
		"props": {
			"label": "Movimento",
			"href": "/movimento",
			"blockClass": ["qual-seu-objetivo-mobile"]
