{
	"flex-layout.row#qual-seu-objetivo": {
		"title": "Row - Qual seu objetivo",
		"children": ["flex-layout.col#qual-seu-objetivo"]
	},

	"flex-layout.col#qual-seu-objetivo": {
		"title": "Col - Qual seu objetivo",
