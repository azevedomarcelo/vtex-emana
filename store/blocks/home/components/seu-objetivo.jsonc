		"title": "Container - Qual seu objetivo",
		"children": [
			"flex-layout.col#content-seu-objetivo1",
			"flex-layout.col#content-seu-objetivo2",
			"flex-layout.col#content-seu-objetivo3",
			"flex-layout.col#content-seu-objetivo4"
		],
		"props": {
			"blockClass": "container-seu-objetivo",
			"fullWidth": true
		}
	},

