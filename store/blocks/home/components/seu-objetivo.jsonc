	"responsive-layout.desktop#qual-seu-objetivo": {
		"children": ["flex-layout.row#content-seu-objetivo"]
	},

	"rich-text#qual-seu-objetivo-title": {
		"title": "Titulo - Qual seu objetivo",
		"props": {
			"text": "QUAL É O __SEU OBJETIVO?__",
			"blockClass": "seu-objetivo"
		}
	},

